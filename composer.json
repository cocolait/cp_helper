{
    "name": "cocolait/cp_helper",
    "description": "A common extension library",
    "type": "library",
    "require": {
        "php": ">=5.5.0",
        "siriusphp/upload": "^2.1",
        "phpoffice/phpexcel": "1.8.*",
        "phpmailer/phpmailer": "^6.0",
        "endroid/qrcode": "1.9.*"
    },
    "license": "mit",
    "authors": [
        {
            "name": "cocolait",
            "email": "enkipen@qq.com"
        }
    ],
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "autoload" : {
        "psr-4" : {
            "cocolait\\helper\\" : "src/"
        },
        "files" : [
            "src/helper.php"
        ]
    }
}
